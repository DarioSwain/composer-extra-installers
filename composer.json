{
  "name": "darioswain/composer-extra-installers",
  "type": "composer-installer",
  "description": "Composer library installer",
  "autoload": {
    "psr-0": { "Composer\\Installers\\": "src/" }
  },
  "extra": {
    "class": "Composer\\Installers\\Installer",
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev"
  }
}